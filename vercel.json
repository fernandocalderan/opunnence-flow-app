{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/demo", "destination": "/demo/index.html" },
    { "source": "/demo/", "destination": "/demo/index.html" },
    { "source": "/demo/(.*)", "destination": "/demo/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
